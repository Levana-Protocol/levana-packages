{
  "name": "@levana-protocol/ui",
  "version": "0.1.2",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup src/**/*.{ts,tsx} --format cjs,esm --dts --watch",
    "build": "tsup src/**/*.{ts,tsx} --format cjs,esm --dts",
    "lint": "biome check"
  },
  "files": [
    "dist/"
  ],
  "exports": {
    "./AdaptiveList": {
      "types": "./src/AdaptiveList.tsx",
      "default": "./dist/AdaptiveList.js"
    },
    "./AdaptiveListHeader": {
      "types": "./src/AdaptiveListHeader.tsx",
      "default": "./dist/AdaptiveListHeader.js"
    },
    "./AdaptiveListOptionsModal": {
      "types": "./src/AdaptiveListOptionsModal.tsx",
      "default": "./dist/AdaptiveListOptionsModal.js"
    },
    "./BreakpointDetection": {
      "types": "./src/BreakpointDetection.tsx",
      "default": "./dist/BreakpointDetection.js"
    },
    "./ButtonSlider": {
      "types": "./src/ButtonSlider/ButtonSlider.tsx",
      "default": "./dist/ButtonSlider/ButtonSlider.js"
    },
    "./Countdown": {
      "types": "./src/Countdown/Countdown.tsx",
      "default": "./dist/Countdown/Countdown.js"
    },
    "./FormFieldContainer": {
      "types": "./src/FormFieldContainer/FormFieldContainer.tsx",
      "default": "./dist/FormFieldContainer/FormFieldContainer.js"
    },
    "./FormInput": {
      "types": "./src/FormInput/FormInput.tsx",
      "default": "./dist/FormInput/FormInput.js"
    },
    "./FormToggleInput": {
      "types": "./src/FormToggleInput/FormToggleInput.tsx",
      "default": "./dist/FormToggleInput/FormToggleInput.js"
    },
    "./GainsTypography": {
      "types": "./src/GainsTypography/GainsTypography.tsx",
      "default": "./dist/GainsTypography/GainsTypography.js"
    },
    "./HelperButton": {
      "types": "./src/HelperButton.tsx",
      "default": "./dist/HelperButton.js"
    },
    "./HorizontalGauge": {
      "types": "./src/HorizontalGauge/HorizontalGauge.tsx",
      "default": "./dist/HorizontalGauge/HorizontalGauge.js"
    },
    "./NavigationModal": {
      "types": "./src/NavigationModal.tsx",
      "default": "./dist/NavigationModal.js"
    },
    "./NavigationModalDialog": {
      "types": "./src/NavigationModalDialog.tsx",
      "default": "./dist/NavigationModalDialog.js"
    },
    "./RiskBars": {
      "types": "./src/RiskBars.tsx",
      "default": "./dist/RiskBars.js"
    },
    "./SettingsInput": {
      "types": "./src/SettingsInput.tsx",
      "default": "./dist/SettingsInput.js"
    },
    "./StackedGainsStats": {
      "types": "./src/StackedGainsStats/StackedGainsStats.tsx",
      "default": "./dist/StackedGainsStats/StackedGainsStats.js"
    },
    "./StackedStats": {
      "types": "./src/StackedStats/StackedStats.tsx",
      "default": "./dist/StackedStats/StackedStats.js"
    },
    "./ThemeProvider": {
      "types": "./src/ThemeProvider/index.tsx",
      "default": "./dist/ThemeProvider/index.js"
    },
    "./icons/*": {
      "types": "./src/icons/*.tsx",
      "default": "./dist/icons/*.js"
    },
    "./mui": {
      "types": "./src/utils/mui.ts",
      "default": "./dist/utils/mui.js"
    },
    "./types": {
      "types": "./src/types.ts",
      "default": "./dist/types.js"
    },
    "./useThemeMode": {
      "types": "./src/useThemeMode.ts",
      "default": "./dist/useThemeMode.js"
    }
  },
  "dependencies": {
    "@emotion/cache": "11.13.1",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@fontsource/barlow-semi-condensed": "5.1.0",
    "@fontsource/montserrat": "5.1.0",
    "@levana-protocol/utils": "0.0.3",
    "@mui/base": "5.0.0-beta.58",
    "@mui/joy": "5.0.0-beta.48",
    "@mui/system": "6.1.3",
    "@mui/types": "7.2.18",
    "@types/sortablejs": "1.15.8",
    "react": "18.3.1",
    "sortablejs": "1.15.2",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@levana-protocol/typescript-config": "0.0.1",
    "@types/react": "18.3.2",
    "tsup": "8.3.5",
    "typescript": "5.4.5"
  }
}
